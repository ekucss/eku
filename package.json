{
    "name": "eku",
    "version": "0.6.0",
    "description": "A friendly style library",
    "main": "src/eku.scss",
    "unpkg": "dist/eku.min.css",
    "repository": "https://github.com/ekucss/eku",
    "homepage": "https://github.com/ekucss/eku#readme",
    "author": "Andres Celis",
    "license": "MIT",
    "keywords": [
        "css",
        "sass",
        "eku",
        "library"
    ],
    "scripts": {
        "minify": "sass src/eku.scss dist/eku.min.css --no-source-map -s compressed",
        "build": "sass src/eku.scss dist/eku.css -s expanded",
        "dev": "sass src/eku.scss dist/eku.css --watch -s expanded"
    },
    "dependencies": {
        "sass": "^1.32.8"
    }
}
